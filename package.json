{
  "name": "lido-csm-sdk",
  "version": "0.0.0-development",
  "main": "index.js",
  "repository": "git@github.com:lidofinance/lido-csm-sdk.git",
  "license": "GPL-3.0-or-later",
  "private": true,
  "keywords": [
    "lido",
    "sdk",
    "csm",
    "ethereum"
  ],
  "scripts": {
    "dev": "yarn playground:dev",
    "w-info": "yarn workspaces info",
    "build": "yarn workspaces foreach -pt run build",
    "build:packages": "yarn workspaces foreach --no-private run build",
    "types": "yarn workspaces foreach -pt run types",
    "test": "yarn workspaces foreach -pt run test",
    "lint": "yarn workspaces foreach -pt run lint",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/js": "^10.0.1",
    "@qiwi/multi-semantic-release": "7.1.2",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.14.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-sonarjs": "^3.0.7",
    "eslint-plugin-unicorn": "^63.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.56.0"
  },
  "resolutions": {
    "postcss": "8.4.31",
    "crypto-js": "4.2.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "release": {
    "branches": [
      "main",
      {
        "name": "develop",
        "channel": "alpha",
        "prerelease": "alpha"
      }
    ]
  },
  "multi-release": {
    "sequentialInit": true,
    "deps": {
      "bump": "override",
      "release": "patch"
    }
  },
  "packageManager": "yarn@3.6.3",
  "engines": {
    "node": ">=20"
  }
}
